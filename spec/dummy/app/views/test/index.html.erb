          </select>
        </div>

        <div class="field">
          <div class="switch">
            <input type="checkbox" name="switch" id="myswitch">
            <label class="ss-icon" for="myswitch">
              <div class="inner"></div>
              <div class="nib"></div>
            </label>
          </div>
          <p class="switch-label">This is my value</p>
        </div>

        <div class="field">
          <label for="">Radio buttons</label>
          <input id="age_child" name="age" type="radio" value="child" />
          <label for="age_child" class="inline">I am younger than 21</label>
          <br/>
          <input id="age_adult" name="age" type="radio" value="adult" />
          <label for="age_adult" class="inline">I'm over 21</label>
        </div>
